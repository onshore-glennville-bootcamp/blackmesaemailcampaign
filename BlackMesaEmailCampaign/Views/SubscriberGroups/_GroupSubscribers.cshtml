@model BLL.SubscribersVM

@if (Session["ID"] == null)
{
    @Html.Action("Index", "Home");
}

<table>
    <thead style="display: block; ">
        <tr>
            <th>Current Subscribers(@Model.Subscribers.Count)</th>
        </tr>
    </thead>
    <tbody class="list">
        @for (int i = 0; i < Model.Subscribers.Count; i++)
        {
            if (Model.Subscribers[i].EmailList)
            {
                <tr style="border: 1px solid #ccc; ">
                    <td>@Model.Subscribers[i].LastName</td>
                    <td>@Model.Subscribers[i].FirstName</td>
                </tr>
            }
        }
    </tbody>
</table>