@model BLL.GroupFM

<h1>Check Subscriber to Add</h1>

<tbody class="list">
    @for (int i = 0; i < Model.Subscribers.Subscribers.Count ; i++)
    {
        <tr style="border: 1px solid #ccc; ">
            <td>@Html.CheckBoxFor(m => m.Subscribers.Subscribers[i].EmailList)</td>
            <td>@Model.Subscribers.Subscribers[i].LastName</td>
            <td>@Model.Subscribers.Subscribers[i].FirstName</td>
        </tr>
    }
</tbody>
